{
  "name": "intertravel-unified-backend",
  "version": "3.2.0-FASE2",
  "description": "InterTravel Backend Completo - Fase 2",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "setup": "node test-setup.js",
    "db:init": "node -e \"require('./database').initializeDatabase()\"",
    "db:test": "node -e \"require('./database').healthCheck().then(r => console.log('DB Health:', r))\""
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mercadopago": "^2.8.0",
    "multer": "^2.0.1",
    "nodemailer": "^7.0.4",
    "pg": "^8.16.3",
    "stripe": "^18.3.0",
    "validator": "^13.15.15",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 30000,
    "verbose": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "routes/**/*.js",
      "middleware/**/*.js",
      "database.js",
      "!routes/**/mock/**",
      "!**/*.test.js",
      "!coverage/**"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "keywords": [
    "intertravel",
    "travel",
    "api",
    "booking",
    "admin",
    "postgresql"
  ],
  "author": "InterTravel Group",
  "license": "ISC"
}